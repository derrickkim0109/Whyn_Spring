<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
	<mapper namespace="com.jsplec.wp.account">
	
	<select id="counselingManagermentList" resultType="com.jsplec.wp.acount.ACLDto">
		SELECT CNO,CUSERNAME,CUSERID,CTITLE,CCONTENT,CDATE FROM COUNSELING
	</select>
	
	<select id="counselingSearch"  parameterType = "String" resultType="com.jsplec.wp.acount.ACLDto">
		SELECT CNO , CUSERNAME, CUSERID, CTITLE,CCONTENT, CDATE FROM COUNSELING WHERE ${param1} LIKE CONCAT('%',#{param2},'%')
	</select>
	
	<delete id="counselingDelete">
		DELETE FROM COUNSELING WHERE CNO = #{param1}
	</delete>
	
	
	<select id="counselingContentView" resultType="com.jsplec.wp.acount.ACLDto">
		SELECT * FROM COUNSELING WHERE CNO = #{param1}
	</select>
	
	<select id="">
	
	
	
	</select>
	
	
	
	</mapper>
	